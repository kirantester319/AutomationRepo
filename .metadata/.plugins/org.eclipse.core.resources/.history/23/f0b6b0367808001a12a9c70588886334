	//Occurrence of a character
	
	String s = "Selenium";
	
	Map<Character, Integer> chm= new HashMap<>();
	
	
			
			
			
	for(Character c : s.toCharArray())
			{
		
		if(chm.containsKey(c))
		{
			chm.put(c, chm.get(c)+1);          
		}
		else
		{
			chm.put(c, 1);
		}
		
		
	}
	
	
	
	System.out.println(chm);
	
	
	
	
	}
	
	
	
	
	
	
	
	
}
