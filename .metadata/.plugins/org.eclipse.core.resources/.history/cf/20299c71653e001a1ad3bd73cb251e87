package polymorphism;

class parent {
	
  void show ()
 {
	  
	 System.out.println("Parent show method");
 }
}


class child extends parent
{
	 public void show()
	{	this.show();
		System.out.println("Chid show method");
	}
	
}	

	
	public class OverrideMethod
	{
	public static void main (String args[])
	
	{
		parent o = new child();
		
		o.show();
		
	/*	child c = new child();
		c.show();
	*/	
	}
}