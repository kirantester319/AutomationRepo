
public class PracticeOfTheDat {
	
	
	
	
	public static void main(String[] args) {
		
		
		int i=1;
		/*while(i<=10)
		{
			
			
			System.out.println(i);
			i++;
			
		}*/
	//	Print odd number
		while (i<=10)
		{
			System.out.println(i);
			i+=2; //i=i+2;		
		}
		
		//Print Even number
		
		i=2;
		while(i<=10)
		{
			System.out.println(i);
			i+=2;
		}
		
		i=10;
		while(i>=10)
		{
			System.out.println(i);
			i--;
		}
	}

}
